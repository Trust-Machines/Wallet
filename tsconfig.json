{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "experimentalDecorators": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@redux/*": ["redux/*"],
      "@screens/*": ["screens/*"],
      "@shared/*": ["shared/*"],
      "@utils/*": ["utils/*"],
    }
  },
}
